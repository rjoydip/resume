{
  "name": "rjoydip-resume",
  "version": "0.1.1",
  "private": true,
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "prepare": "husky && playwright install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix && stylelint \"**/*.css\" --fix",
    "test:unit": "vitest run --typecheck --mode unit",
    "test:e2e": "vitest run --typecheck --mode e2e",
    "test:bench": "vitest bench --run && tsx ./test/benchmark/endpoints/index.ts",
    "test:ci": "nr test:unit && start-server-and-test 'nr start' http://localhost:3000 'nr test:e2e && nr test:bench'",
    "deps:update": "taze -r -w -i -f -l"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@vercel/blob": "^0.23.3",
    "@vercel/edge-config": "^1.1.1",
    "@vercel/speed-insights": "^1.0.10",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "hono": "^4.3.10",
    "humanize-string": "^3.0.0",
    "lucide-react": "^0.372.0",
    "next": "^14.2.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtuoso": "^4.7.10",
    "scroll-data-hook": "^1.0.4",
    "std-env": "^3.7.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tinydate": "^1.3.0",
    "uid": "^2.0.2",
    "valibot": "^0.30.0",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@eslint-react/eslint-plugin": "^1.5.12",
    "@million/lint": "1.0.0-rc.7",
    "@ngneat/falso": "^7.2.0",
    "@testing-library/react": "^15.0.7",
    "@types/autocannon": "^7.12.5",
    "@types/k6": "^0.51.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/browser": "^1.6.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "autocannon": "^7.15.0",
    "autoprefixer": "^10.4.19",
    "consola": "^3.2.3",
    "envinfo": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "happy-dom": "^14.11.0",
    "husky": "^9.0.11",
    "imagemin-lint-staged": "^0.5.1",
    "lint-staged": "^15.2.4",
    "msw": "^2.3.0",
    "playwright": "^1.44.1",
    "pnpm": "^9.1.2",
    "postcss": "^8.4.38",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.3",
    "taze": "^0.13.8",
    "tsup": "^8.0.2",
    "tsx": "^4.11.0",
    "turbo": "^1.13.3",
    "typescript": "^5.4.5",
    "vercel": "^34.2.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yaml}": "eslint --fix",
    "*.{css}": "stylelint --fix",
    "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-tailwindcss"
    ]
  },
  "msw": {
    "workerDirectory": [
      "test\\mocks"
    ]
  }
}
