{
  "name": "resume",
  "version": "0.6.0",
  "private": true,
  "packageManager": "bun@1.2.8",
  "scripts": {
    "prepare": "husky && playwright install",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "bun run test:dom && bun run test:node && bun run test:e2e",
    "test:e2e": "playwright test",
    "test:dom": "vitest run --mode dom --environment=happy-dom",
    "test:node": "vitest run --mode node --environment=node"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.71.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.486.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "std-env": "^3.8.1",
    "tailwind-merge": "^3.1.0",
    "titleize": "^4.0.0",
    "valibot": "^0.30.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@eslint-react/eslint-plugin": "^1.40.2",
    "@lukeed/uuid": "^2.0.1",
    "@ngneat/falso": "^7.3.0",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4.1.1",
    "@tanstack/react-query-devtools": "^5.71.5",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fast-xml-parser": "^5.2.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "imagemin-lint-staged": "^0.5.1",
    "lint-staged": "^15.5.0",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.1",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "vercel": "^41.4.1",
    "vitest": "^3.1.1",
    "vitest-dom": "^0.1.1"
  },
  "lint-staged": {
    "*.{css,js,jsx,ts,tsx,json,md,yaml}": "eslint --fix",
    "*.{png,jpeg,jpg,gif,svg}": "imagemin-lint-staged"
  }
}
