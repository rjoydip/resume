    "autoprefixer": "^10.4.19",
    "cypress": "^13.8.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
