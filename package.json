{
  "name": "rjoydip-resume",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.7",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "e2e:ci": "del-cli cypress/report/* && cypress run --e2e && cypress run --component && pnpm e2e:report",
    "e2e:report": "mochawesome-merge cypress/report/*.json -o cypress/report/html/mochawesome-merged.json && marge cypress/report/html/mochawesome-merged.json -o cypress/report/html",
    "e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "deps:update": "taze -r -w -i -f -l",
    "prepare": "husky"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.16.0",
    "@radix-ui/react-avatar": "^1.0.4",
