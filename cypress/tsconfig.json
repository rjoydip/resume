{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es2015",
      "dom"
    ],
    "types": [
      "cypress"
    ],
    "allowJs": true,
    "strictNullChecks": true,
    "noEmit": true,
    "isolatedModules": false,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "./cypress.d.ts",
    "**/*.cy.ts",
    "**/*.cy.tsx"
  ],
  "exclude": []
}
